  "version": "0.2.0",
  "configurations": [
    {
      "type": "extensionHost",
      "request": "launch",
      "name": "Launch Extension",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/git-temporal-vscode"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/git-temporal-vscode/out/**/*.js"
      ]
    }
  ]
}
