{
	"name": "git-temporal",
	"description": "Git Temporal - all about time and time travel",
	"version": "0.0.1",
	"publisher": "littlebee",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:gitTemporal.start"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "gitTemporal.start",
				"title": "Open Git Temporal",
				"category": "git-temporal"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run build",
		"build": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"vscode": "^1.1.18"
	},
	"devDependencies": {
		"@types/node": "^10.5.2",
		"tslint": "^5.11.0",
		"typescript": "^2.9.2"
	}
}
